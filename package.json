    "vue",
    "element",
    "form-create"
  ],
  "files": [
    "dist"
  ],
  "author": "aylanbrown",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aylanbrown/element-form-create/issues"
  },
  "homepage": "https://github.com/aylanbrown/element-form-create#readme",
